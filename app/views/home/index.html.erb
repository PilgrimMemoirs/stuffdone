<div class="page-header">
  <h1>My Feats!</h1>
</div>
 
<div id="my-accomplishments">
	<%= @accomplishments.each do |accomplishment| %>
	  <div>
	  <h2><accomplishment.title</h2>
	 
	  <small><em><%= accomplishment.timestamp %></em></small>
	 
	  <p><%= truncate(strip_tags(accomplishment.desc), length: 600) %></p>
	</div>
	</div>
	<% end %>
 
<div id="infinite-scrolling">
  <%= will_paginate %>
</div>